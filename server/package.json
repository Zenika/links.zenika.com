{
  "name": "links-zenika-com-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon --watch src/**/*.js --watch .env --exec dotenv node src/server.js",
    "lint": "eslint src/**/*.js",
    "lint:strict": "npm run lint -- --max-warnings=0",
    "prettier": "prettier *.json src/**/*.{js,json,graphql}",
    "prettier:check": "npm run prettier -- --check",
    "prettier:write": "npm run prettier -- --write"
  },
  "devDependencies": {
    "dotenv-cli": "^3.1.0",
    "eslint": "^6.8.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "node-fetch": "^2.6.0",
    "redirect-ssl": "^1.4.1",
    "ua-parser-js": "^0.7.21"
  }
}
